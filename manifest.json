{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "#wrap",
  "version": "2.3.3",
  "manifest_version": 2,
  "description": "Allows Tumblr tags to wrap around without having to use the awful drag-scrolling interface.",
  "icons": { "16": "icon_16.png",
             "48": "icon_48.png",
             "128": "icon_128.png" },
  "content_scripts": [
    {
      "matches": ["http://*.tumblr.com/*", "https://*.tumblr.com/*"],
      "css": ["main.css"]
    }
  ]
}